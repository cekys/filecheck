{
    "log": {
        "logLocation": "./log",
        "reportLocation": "./report"
    },
    "targets": [
        {
            "name": "log",
            "description": "log",
            "location": "./",
            "mode": "sha1",
            "filter": [
                "_Deleted",
                "System Volume Information",
                "$Recycle.Bin",
                "C:/Windows",
                "\\.(log|123)$"
            ]
        }
    ]
}